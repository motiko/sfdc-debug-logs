{
  "name": "",
  "description": "ff extension",
  "version": "0.0.1",
  "author": "motiko",
  "license": "MIT",
  "main": "",
  "private": true,
  "scripts": {
    "build": "browserify -g [ envify --NODE_ENV production ] -t [ babelify  ] -t [ browserify-css --global ] -g [ uglifyify ] src/*.js | uglifyjs --compress --mangle | tee ../firefox/app/js/build/app.js ../chrome/app/js/build/app.js > /dev/null",
    "watch": "watchify -t [ babelify ]  -t [ browserify-css --global  ] src/*.js -o '| tee ../firefox/app/js/build/app.js ../chrome/app/js/build/app.js > /dev/null'",
    "clean": "rm ../firefox/app/js/build/app.js ../chrome/app/js/build/app.js",
    "react-devtools": "react-devtools",
    "test": "standard"
  },
  "dependencies": {
    "browserify": "^16.3.0",
    "idb-keyval": "^2.3.0",
    "material-ui": "^1.0.0-beta.24",
    "material-ui-color-picker": "github:motiko/material-ui-color-picker#controlled",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.1",
    "rc-slider": "^8.6.1",
    "rc-tooltip": "^3.7.0",
    "react": "^16.2.0",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "redux": "^3.7.2",
    "redux-localstorage": "^1.0.0-rc5",
    "redux-localstorage-filter": "^0.1.1",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.11.0",
    "watchify": "^3.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify-css": "^0.15.0",
    "envify": "^4.1.0",
    "prettier": "^1.11.1",
    "reactify": "^1.1.1",
    "uglify-js": "^3.3.16",
    "uglifyify": "^4.0.5"
  }
}
